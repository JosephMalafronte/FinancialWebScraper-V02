{
	"manifest_version": 2,
	"name": "Financial Analysis",
	"description": "Quick Launch Financial Analysis",
	"version": "1.0.0",
	"icons": {"128": "icon_128.png"},
	"browser_action":{
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"background": {
      "scripts": ["popup.js"],
      "persistent": false
    },
	"permissions": ["activeTab", "declarativeContent", "storage"]
}